<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mp.kafa.mycollege">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Admin"
            android:label="@string/title_activity_admin" />
        <activity android:name=".Student" />
        <activity android:name=".Teacher" />
        <activity android:name=".TimeTable" />
        <activity android:name=".TimeTab" />
        <activity android:name=".AddAttendance" />
        <activity android:name=".ViewAttendance" />
        <activity android:name=".AddInternalMarks" />
        <activity android:name=".ViewInternalMark" />
        <activity android:name=".AdminHome" />
        <activity android:name=".ViewTeachers" />
        <activity android:name=".ViewStudents"></activity>
    </application>

</manifest>